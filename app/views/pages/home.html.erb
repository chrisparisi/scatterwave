<section class="hero-home">
  <div class="swiper-container hero-slider">
    <div class="swiper-wrapper dark-overlay">
      <div style="background-image:url(<%= asset_url('banner.jpg') %>)" class="swiper-slide"></div>
      <div style="background-image:url(<%= asset_url('banner.jpg') %>)" class="swiper-slide"></div>
      <div style="background-image:url(<%= asset_url('banner.jpg') %>)" class="swiper-slide"></div>
      <div style="background-image:url(<%= asset_url('banner.jpg') %>)" class="swiper-slide"></div>
    </div>
  </div>
  <div class="container py-6 py-md-7 text-white z-index-20">
    <div class="row">
      <div class="col-xl-10">
        <div class="text-center text-lg-left">
          <h1 class="subtitle display-4 letter-spacing-4 mb-2 text-secondary text-shadow">Scatterwave</h1>
          <p class="h2 font-weight-bold text-shadow"> Find the bands, shows, and venues in your area</p>
        </div>
        <div class="search-bar mt-5 p-3 p-lg-1 pl-lg-4">
          <%= form_tag search_path do |f| %>
            <div class="row">
              <div class="col-lg-7 d-flex align-items-center form-group">
                <div class="input-label-absolute input-label-absolute-right w-100">
                  <label for="location" class="label-absolute"><i class="fa fa-crosshairs"></i><span class="sr-only">City</span></label>
                  <input type="text" name="search[location]" placeholder="Location" id="showlocation" class="form-control border-0 shadow-0">
                </div>
              </div>
              <div class="col-lg-3 d-flex align-items-center form-group no-divider">
                <select title="Categories" data-style="btn-form-control" class="selectpicker" name="category">
                  <option value="shows" selected>Shows</option>
                  <option value="bands">Bands</option>
                  <option value="venues">Venues</option>
                  <!--<option value="x-large">Garages</option>-->
                </select>
              </div>
              <div class="col-lg-2">
                <button type="submit" class="btn btn-primary btn-block rounded-xl h-100">Search </button>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</section>


<section class="py-5 bg-gray-100">
  <div class="container">
    <div class="row mb-2">
      <div class="col-md-8">
        <p class="subtitle text-secondary">Find great shows playing locally</p>
        <h2>Shows</h2>
      </div>
      <div class="col-md-4 d-lg-flex align-items-center justify-content-end">
        <%= link_to search_shows_path, class: "text-muted text-sm" do %>
            See all Shows<i class="fas fa-angle-double-right ml-2"></i>
        <% end %>
      </div>
    </div>
    <div data-swiper="{&quot;slidesPerView&quot;:3}" class="swiper-container swiper-container-mx-negative swiper-init pt-3">
      <div class="swiper-wrapper pb-5">
        <%#= render partial: "bookings/bookings_list", locals: {bookings: @bookings} %>
        <%= render partial: "bookings/bookings", locals: {shows: @bookings} %>
      </div>
    </div>
  </div>
</section>

<section class="py-5 bg-gray-100">
  <div class="container">
    <div class="row mb-2">
      <div class="col-md-8">
        <p class="subtitle text-secondary">Discover up and coming local bands</p>
        <h2>Bands</h2>
      </div>
      <div class="col-md-4 d-lg-flex align-items-center justify-content-end">
        <%= link_to search_bands_path, class: "text-muted text-sm" do %>
            See all Bands<i class="fas fa-angle-double-right ml-2"></i>
        <% end %>
      </div>
    </div>
    <div data-swiper="{&quot;slidesPerView&quot;:3}" class="swiper-container swiper-container-mx-negative swiper-init pt-3">
      <div class="swiper-wrapper pb-5">
        <%#= render partial: "bands/home_list", locals: {bands: @bands} %>
        <%= render partial: "bands/bands", locals: {bands: @bands} %>
      </div>
    </div>
  </div>
</section>

<section class="py-5 bg-gray-100">
  <div class="container">
    <div class="row mb-2">
      <div class="col-md-8">
        <p class="subtitle text-secondary">Book awesome venues for your band</p>
        <h2>Venues</h2>
      </div>
      <div class="col-md-4 d-lg-flex align-items-center justify-content-end">
        <%= link_to search_venues_path, class: "text-muted text-sm" do %>
            See all Venues<i class="fas fa-angle-double-right ml-2"></i>
        <% end %>
      </div>
    </div>
    <div data-swiper="{&quot;slidesPerView&quot;:3}" class="swiper-container swiper-container-mx-negative swiper-init pt-3">
      <div class="swiper-wrapper pb-5">
        <%#= render partial: "venues/venues_list", locals: {venues: @venues} %>
        <%= render partial: "venues/venues", locals: {venues: @venues} %>
      </div>
    </div>
  </div>
</section>







<script>
    $('#date').datepicker({
        dateFormat: 'dd-mm-yy',
        minDate: 0,
        maxDate: '6m',
    });

    $(function () {
        $("#showlocation").geocomplete();
    });

    $(function () {
        $("#bandlocation").geocomplete();
    });

    $(function () {
        $("#venuelocation").geocomplete();
    })
</script>




























<section class="py-6 bg-gray-100">
  <div class="container">
    <div class="text-center pb-lg-4">
      <p class="subtitle text-secondary">Bringing bands, venues, and audiences together</p>
      <h2 class="mb-5">Find and create amazing concerts</h2>
    </div>
    <div class="row">
      <div class="col-lg-4 mb-3 mb-lg-0 text-center">
        <div class="px-0 px-lg-3">
          <div class="icon-rounded bg-primary-light mb-3">
            <svg class="svg-icon text-primary w-2rem h-2rem">
              <use xlink:href="#destination-map-1"></use>
            </svg>
          </div>
          <h3 class="h5">Shows</h3>
          <p class="text-muted">Find great shows playing in your area. Sort them by genre and area. Then listen to them
            on Scatterwave to see how you like them!</p>
        </div>
      </div>
      <div class="col-lg-4 mb-3 mb-lg-0 text-center">
        <div class="px-0 px-lg-3">
          <div class="icon-rounded bg-primary-light mb-3">
            <svg class="svg-icon text-primary w-2rem h-2rem">
              <use xlink:href="#pay-by-card-1"></use>
            </svg>
          </div>
          <h3 class="h5">Bands</h3>
          <p class="text-muted">Find and book your next tour. Search venues for available time slots, see what they are
            paying and book them on the spot. Then connect with audiences on Scatterwave to insure you get great
            audiences!</p>
        </div>
      </div>
      <div class="col-lg-4 mb-3 mb-lg-0 text-center">
        <div class="px-0 px-lg-3">
          <div class="icon-rounded bg-primary-light mb-3">
            <svg class="svg-icon text-primary w-2rem h-2rem">
              <use xlink:href="#heart-1"></use>
            </svg>
          </div>
          <h3 class="h5">Venues</h3>
          <p class="text-muted">Simplify the booking process! Select a time you are looking for a band and bands will
            submit their electronic press kits to you, then select the band that is the best fit for your venue. Booked
            shows will be promoted to local audiences making sure everybody knows about your events. </p>
        </div>
      </div>
    </div>
  </div>
</section>
